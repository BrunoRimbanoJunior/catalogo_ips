VITE_DEFAULT_MANIFEST_URL=https://raw.githubusercontent.com/<org>/<repo>/main/manifest.json

# R2 (opcional, apenas para gerar manifest via script/CI)
# Preencha para usar scripts/gen-manifest-r2.mjs
R2_ACCOUNT_ID=
R2_BUCKET=
R2_ACCESS_KEY_ID=
R2_SECRET_ACCESS_KEY=
# Se não informar, o script monta a partir do ACCOUNT_ID
R2_ENDPOINT=
# Base pública (usada no manifest); se vazio, o script usa <endpoint>/<bucket>/
R2_PUBLIC_BASE_URL=
